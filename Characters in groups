#include<stdio.h>
int main()
{
    char ch[100];
    scanf("%s",ch);
    int count =0;
    for(int i =0; ch[i] != '\0'; i++)
        count++;
    int num;
    scanf(" %d", &num);
    int k =0, j;
    for(int i = k; i < count; i++)
    {
      for( j =0; j < num; j++)
      {  
          if(ch[k] != '\0')
          {
              printf("%c", ch[k]);
              k++;
          }
      }
      if(j == num && ch[k] != '\0')
          printf("%c", 32);
    }
 
}
