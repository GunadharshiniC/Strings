#include<stdio.h>
#include<string.h>
int main()
{
    char str[200];
    scanf("%s",str);
    int n1 = strlen(str);
    char str1[200];
    scanf("%s",str1);
    int n2 = strlen(str1);
    for(int i=0;i<n1+n2;i++)
    {
        if(i<n1)
            printf("%c",str[i]);
        if(i<n2)
            printf("%c",str1[i]); 
    }
}
